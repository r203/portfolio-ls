.parallax {
    position: relative;
    overflow: hidden;
    height: 1500PX; // какое число тут подставлять???
    &__image {
    object-fit: cover;
    position: absolute;
    width: 100%;
    height: 100%;
    }
    &__layer {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }
}